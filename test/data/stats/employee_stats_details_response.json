{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "data": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "employee id"
                },
                "position": {
                    "type": "string",
                    "description": "employee position"
                },
                "employee_type": {
                    "type": "string"
                },
                "party": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "first_name": {
                            "type": "string"
                        },
                        "second_name": {
                            "type": "string"
                        },
                        "last_name": {
                            "type": "string"
                        },
                        "educations": {
                            "type": "array",
                            "description": "освіта"
                        },
                        "qualifications": {
                            "type": "array",
                            "description": "підвищення кваліфікації"
                        },
                        "specialities": {
                            "type": "array",
                            "description": "блок інформації про отримання сертифікату спеціалісту та проходження атестацій"
                        },
                        "is_available": {
                            "type": "boolean"
                        },
                        "science_degree": {
                            "type": "object",
                            "properties": {
                                "country": {
                                    "type": "string",
                                    "description": "`Dictionary COUNTRY`"
                                },
                                "city": {
                                    "type": "string"
                                },
                                "degree": {
                                    "type": "string",
                                    "description": "`Dictionary SCIENCE_DEGREE`"
                                },
                                "institution_name": {
                                    "type": "string"
                                },
                                "diploma_number": {
                                    "type": "string"
                                },
                                "speciality": {
                                    "type": "string"
                                },
                                "issued_date": {
                                    "type": "string",
                                    "description": "02-28 (string, required)"
                                }
                            },
                            "required": [
                                "country",
                                "city",
                                "degree",
                                "institution_name",
                                "diploma_number",
                                "speciality"
                            ]
                        },
                        "working_experience": {
                            "type": [
                                "number",
                                "null"
                            ]
                        },
                        "about_myself": {
                            "type": [
                                "string",
                                "null"
                            ]
                        }
                    },
                    "required": [
                        "id",
                        "first_name",
                        "second_name",
                        "last_name",
                        "educations",
                        "specialities",
                        "is_available"
                    ]
                },
                "division": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "name": {
                            "type": "string"
                        },
                        "type": {
                            "type": "string",
                            "enum": [
                                "CLINIC",
                                "AMBULANT_CLINIC",
                                "FAP"
                            ]
                        },
                        "addresses": {
                            "type": "object",
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "description": "`Dictionary ADDRESS_TYPE`. As for now Regisrtration and Residence types"
                                },
                                "country": {
                                    "type": "string",
                                    "enum": [
                                        "UA"
                                    ],
                                    "description": "`Dictionary COUNTRY`"
                                },
                                "area": {
                                    "type": "string",
                                    "description": "one of Ukraianian area"
                                },
                                "region": {
                                    "type": "string",
                                    "description": "district of area"
                                },
                                "settlement": {
                                    "type": "string",
                                    "description": "city name"
                                },
                                "settlement_type": {
                                    "type": "string",
                                    "description": "`Dictionary SETTLEMENT_TYPE` - type of settlement as city/town/village etc"
                                },
                                "settlement_id": {
                                    "type": "string",
                                    "description": "settlement identificator from uaadresses"
                                },
                                "street_type": {
                                    "type": "string",
                                    "description": "`Dictionary STREET_TYPE` - type of street as street/road/line etc"
                                },
                                "street": {
                                    "type": "string",
                                    "description": "street name"
                                },
                                "building": {
                                    "type": "string",
                                    "description": "number of building"
                                },
                                "apartment": {
                                    "type": "string",
                                    "description": "number of appartment"
                                },
                                "zip": {
                                    "type": "string",
                                    "description": "system of postal codes"
                                }
                            },
                            "required": [
                                "type",
                                "country",
                                "settlement_id",
                                "building"
                            ]
                        },
                        "legal_entity": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string"
                                },
                                "name": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "id",
                                "name"
                            ]
                        }
                    },
                    "required": [
                        "id",
                        "name",
                        "type",
                        "legal_entity"
                    ]
                }
            },
            "required": [
                "id",
                "position",
                "employee_type",
                "division"
            ]
        }
    }
}
